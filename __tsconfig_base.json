{
  "compilerOptions": {
    "target": "es6",
    "module": "amd",   
    "lib": ["dom", "es6"],    
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "strictFunctionTypes": true, 
    "removeComments": true,
    "skipLibCheck": true
  }  
}
